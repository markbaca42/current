extends layout.pug
    
block content
    h1 #{user.id} you made it so listen to #{songs[0].name}

     table 
      each t in songs
        tr 
          - var artist_name = t.artists[0].name
          - var track_name = t.name
          - var track_preview = t.preview_url

          td #{artist_name}
          td #{track_name}
          td: a(href=track_preview) Preview
    
    